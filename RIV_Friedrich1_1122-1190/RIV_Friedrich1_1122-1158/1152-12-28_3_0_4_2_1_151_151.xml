			<relevantPersonal>
				
					<issuer><persName>Friedrich I.</persName></issuer>
				
				
					<recipient>H. von <hi rend="spaced">Thiel</hi>  <hi rend="spaced">Floreffe</hi></recipient>
				
				
				
