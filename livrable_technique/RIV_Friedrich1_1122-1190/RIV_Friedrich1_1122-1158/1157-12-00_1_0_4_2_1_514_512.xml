					<issuer><persName>Friedrich I.</persName></issuer>
				
				
					<recipient><hi rend="spaced">Albrecht</hi>  <hi rend="spaced">Bär</hi>  <hi rend="spaced">Leitzkau</hi></recipient>
				
				
				
				
				
