					<issuer><persName>Friedrich I.</persName></issuer>
				
				
					<recipient><hi rend="spaced">Albert</hi>  <hi rend="spaced">La-Tour-du-Pin</hi>  <hi rend="spaced">Coligny</hi></recipient>
				
				
				
				
				
